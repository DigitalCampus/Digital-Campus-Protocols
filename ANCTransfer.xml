			<bind id="startTime" nodeset="/data/startTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
			<bind id="endTime" nodeset="/data/endTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
