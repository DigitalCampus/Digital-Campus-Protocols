			<bind id="q_birthinterval" nodeset="/data/q_birthinterval" type="select1" required="true()" relevant="/data/q_parity &gt; 1"/>
			<bind id="q_livebirths" nodeset="/data/q_livebirths" type="integer" required="true()" relevant="/data/q_gravida &gt; 1" constraint=". &gt;= 0 and . &lt;= /data/q_gravida"
				jr:constraintMsg="jr:itext('constraint_zerotototalpreg')"/>
			<bind id="q_stillbirths" nodeset="/data/q_stillbirths" type="integer" required="true()" relevant="/data/q_gravida &gt; 1" constraint=". &gt;= 0 and . &lt;= /data/q_gravida"
