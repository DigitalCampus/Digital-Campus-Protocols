			<bind id="q_age" nodeset="/data/q_age" required="true()" type="integer" constraint="((now() div 365.25) - (/data/q_yearofbirth -1962)) &lt; (. + 4) and ((now() div 365.25) - (/data/q_yearofbirth -1962)) &gt; (. - 3)" jr:constraintMsg="jr:itext('q_age_constraint')" />
			<bind id="q_username" nodeset="/data/q_username" required="true()" type="string" />

			<bind id="infosection2" nodeset="/data/infosection2" readonly="true()" type="string"/>
			<bind id="q_gravida" nodeset="/data/q_gravida" type="integer" required="true()" constraint=". &gt;= 1" jr:constraintMsg="jr:itext('q_gravida_constraint')"/>
			<bind id="q_parity" nodeset="/data/q_parity" type="integer" required="true()" relevant="/data/q_gravida &gt; 1" constraint=". &gt;= 0 and . &lt;= /data/q_gravida"
