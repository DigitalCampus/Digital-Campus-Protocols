	<h:head>
		<Meta>
			<UserID/>
			<DeviceID/>
			<TimeStartRecorded/>
			<TimeEndRecorded/>
		</Meta>
		<h:title>Termination (ምቁራፅ ጥንሲ) v1</h:title>
		<model>
			<itext>
				<translation lang="English">
					<text id="yes"><value>Yes</value></text>
					<text id="no"><value>No</value></text>
					<text id="notsure"><value>Not sure</value></text>
					<text id="dontknow"><value>Don't know</value></text>
					<text id="selectone"><value>Select one</value></text>
					<text id="selectall"><value>Select all that apply</value></text>
					<text id="checklist_text"><value>Checklist</value></text>
					<text id="thanks_text"><value>Thank the woman for their cooperation and end the session.</value></text>
					
					<text id="infoprotocol_text"><value>This protocol should be filled out when a pregnancy has ended prematurely or unexpectedly.</value></text>
					<text id="q_consent_text"><value>Q101. Do you give me your consent to use phone for interview?</value></text>
					<text id="infoconsent_text"><value>Put phone down and provide care as normal</value></text>
					<text id="q_healthpointid_text"><value>Q102. Select where the user was registered</value></text>
					<text id="q_healthpointid_9999"><value>Practice only</value></text>
					<text id="q_healthpointid_1006"><value>Arasegeda health post</value></text>
					<text id="q_healthpointid_1001"><value>Ayanlem health post</value></text>
					<text id="q_healthpointid_1009"><value>Frewoyni health post</value></text>
					<text id="q_healthpointid_1002"><value>Gemad health post</value></text>
					<text id="q_healthpointid_1007"><value>Hagerselam health post</value></text>
					<text id="q_healthpointid_1005"><value>Maynebri health post</value></text>
					<text id="q_healthpointid_1008"><value>Mesanu - Hintalowajerat health post</value></text>
					<text id="q_healthpointid_1000"><value>Mesanu - Kilteawelaelo health post</value></text>
					<text id="q_healthpointid_1003"><value>Adiekli health post</value></text>
					<text id="q_healthpointid_1004"><value>Tsadanaele health post</value></text>
					<text id="q_healthpointid_50001"><value>Abraha Atsbha health centre</value></text>
					<text id="q_healthpointid_50002"><value>Adigudom health centre</value></text>
					<text id="q_healthpointid_50003"><value>Agulae health centre</value></text>
					<text id="q_healthpointid_50004"><value>Hiwane health centre</value></text>
					<text id="q_healthpointid_50005"><value>Negash health centre</value></text>
					<text id="q_healthpointid_70001"><value>Ayder Referal Hospital</value></text>
					
					<text id="q_userid_text"><value>Q103. Enter the user's ID </value></text>
					<text id="q_userid_constraint"><value>Must be greater than 0</value></text>
					
					<text id="q_username_text"><value>Q104. Enter user's name</value></text>
					<text id="q_username_hint"><value>Please enter in Tigrinyan text</value></text>
					
					<text id="q_yearofbirth_text"><value>Q105. Please enter user's year of birth</value></text>
					<text id="q_yearofbirth_hint"><value>Please use the Ethiopian calendar</value></text>
					<text id="q_yearofbirth_constraint"><value>Please enter a year greater than 1900</value></text>
					
					<text id="q_age_text"><value>Q106. Enter user's age</value></text>
					<text id="q_age_constraint"><value>The age does not appear to be consistent with the year of birth, please check.</value></text>
					

					<text id="infosection2_text"><value>Section 2: Pregnancy termination data</value></text>
					
					<text id="q_terminationtype_text"><value>Q201. Termination type:</value></text>
					<text id="q_terminationtype_abortion_spont"><value>Spontaneous abortion (miscarriage)</value></text>
					<text id="q_terminationtype_abortion_elect"><value>Elective abortion</value></text>
					<text id="q_terminationtype_abortion_unsafe"><value>Induced abortion (unsafe)</value></text>
					<text id="q_terminationtype_mat_death"><value>Maternal death</value></text>
					
					<text id="q_terminationdate_text"><value>Q202. Termination date:</value></text>
					<text id="q_terminationdate_constraint"><value>Please enter a date before or equal to today</value></text>
					
					<text id="q_procedure_text"><value>Q203. Procedure:</value></text>
					<text id="q_procedure_notapplic"><value>N/A (not applicable)</value></text>
					<text id="q_procedure_dilation"><value>D &amp; C (Dilation &amp; curettage)</value></text>
					<text id="q_procedure_vacuum"><value>MVA (manual vacuum aspiration)</value></text>
					<text id="q_procedure_misoprostol"><value>Misoprostol given</value></text>

					<text id="q_majorcomplicat_text"><value>Q204. Major Complications:</value></text>
					<text id="q_majorcomplicat_bleed"><value>Bleeding</value></text>
					<text id="q_majorcomplicat_sepsis"><value>Sepsis/infection</value></text>
					<text id="q_majorcomplicat_eclampsia"><value>Eclampsia</value></text>
					<text id="q_majorcomplicat_perforations"><value>Perforations</value></text>
					<text id="q_majorcomplicat_none"><value>None</value></text>
					
					<text id="q_referral_text"><value>Q205. Was the woman referred to a health centre or hospital?</value></text>
					<text id="q_fpcounseled_text"><value>Q206. Was family planning counseled? </value></text>
					<text id="q_fpaccepted_text"><value>Q207. Was family planning accepted?? </value></text>
					<text id="q_comments_text"><value>Q208. Comments</value></text>
					
					<text id="q_gpsdata_text"><value>Q301. Please record the location (using GPS)</value></text>

				</translation>
				<translation lang="Tigrinyan">
					<text id="yes"><value>እወ</value></text>
					<text id="no"><value>ኣይፋሉን</value></text>
					<text id="notsure"><value>ርግፀኛ ኣይኮንኩን</value></text>
					<text id="dontknow"><value>ኣይፈልጥን</value></text>
					<text id="selectone"><value>ሓደ ምረፂ</value></text>
					<text id="selectall"><value>ካብ ሓደ ንላዕሊ መልሲ ክህሉዎ ይኽእል እዩ</value></text>
					<text id="checklist_text"><value>መረጋገፂ ዝርዝር</value></text>
					<text id="thanks_text"><value>እታ ኣዶ ንዝገበረትልኪ ምትሕብባር ኣመስጊንኪ ናይ ሎሚ ክትትልኪ ፈፅሚ።</value></text>
					
					<text id="infoprotocol_text"><value>This protocol should be filled out when a pregnancy has ended prematurely or unexpectedly.</value></text>
					<text id="q_consent_text"><value>ሕቶ 101፡ እዛ ተንቀሳቓሲት ስልኪ ንቓለ መሕትት ንክጥቀመላ ፍቓደኛ ዲኺ?</value></text>
					<text id="infoconsent_text"><value>ተጠቃሚት ግልጋሎት ጥዕና ፍቓደኛ እንድሕር ዘይኮይና ፣እታ ተንቀሳቓሲት ስልኪ ኣቕሚጥኪ እቲ ልሙድ ዝኾነ ግልጋሎት ጥዕና ብወረቐት ሃቢያ፡፡</value></text>
					
					<text id="q_healthpointid_text"><value>ሕቶ102 ፡ እትሰርሕሉ ጥዕና ኬላ/ ጣብያ መዝግቢ</value></text>
					<text id="q_healthpointid_9999"><value>ልምምድ</value></text>
					<text id="q_healthpointid_1006"><value>ዓራሰገዳ ጥዕና ኬላ </value></text>
					<text id="q_healthpointid_1001"><value>ኣይናለም ጥዕና ኬላ</value></text>
					<text id="q_healthpointid_1009"><value>ፍረወይኒ ጥዕና ኬላ</value></text>
					<text id="q_healthpointid_1002"><value>ገማድ  ጥዕና ኬላ</value></text>
					<text id="q_healthpointid_1007"><value>ሃገረሰላም ጥዕና ኬላ</value></text>
					<text id="q_healthpointid_1005"><value>ማይነብሪ ጥዕና ኬላ</value></text>
					<text id="q_healthpointid_1008"><value>መሳኑ (ሕንጣሎ ዋጅራት) ጥዕና ኬላ</value></text>
					<text id="q_healthpointid_1000"><value>መሳኑ (ክልተ ኣውላዕሎ) ጥዕና ኬላ</value></text>
					<text id="q_healthpointid_1003"><value>ዓዲእኽሊ ጥዕና ኬላ </value></text>
					<text id="q_healthpointid_1004"><value>ፃዕዳ ናዕለ ጥዕና ኬላ</value></text>
					<text id="q_healthpointid_50001"><value>ኣብርሃ ወኣፅባሃ ጥዕና ጣብያ</value></text>
					<text id="q_healthpointid_50002"><value>ዓዲጉዶም ጥዕና ጣብያ</value></text>
					<text id="q_healthpointid_50003"><value>ኣጉላዕ ጥዕና ጣብያ</value></text>
					<text id="q_healthpointid_50004"><value>ሕዋነ ጥዕና ጣብያ</value></text>
					<text id="q_healthpointid_50005"><value>ነጋሽ ጥዕና ጣብያ</value></text>
					<text id="q_healthpointid_70001"><value>ዓይደር ሪፈራል ሆስፒታል </value></text>
					
					<text id="q_userid_text"><value>ሕቶ103 ፡ መለለይ ቑፅሪ ተጠቃሚት ግልጋሎት ጥዕና መዝግቢ </value></text>
					<text id="q_userid_constraint"><value>ካብ 0 ንላዕሊ ክኸውን ኣለዎ</value></text>
					<text id="q_username_text"><value>ሕ104፡ ሽም ተጠቃሚ</value></text>
					<text id="q_username_hint"><value>ብትግርኛ ይፀሓፍ</value></text>
					<text id="q_yearofbirth_text"><value>ሕቶ 104 ፡ ተጠቃሚት ግልጋሎት ጥዕና ዝተወለደትሉ ዓ/ም መዝግቢ።</value></text>
					<text id="q_yearofbirth_hint"><value>ብአቆፃፅራ ኢትዮጵያ ይፀሓፍ</value></text>
					<text id="q_yearofbirth_constraint"><value>ካብ 1900 ዓ/ም ንላዕሊ ክኸውን ኣለዎ</value></text>
					<text id="q_age_text"><value>ሕቶ 106 ፡ ዕድመ ተጠቃሚት ግልጋሎት ጥዕና?</value></text>
					<text id="q_age_constraint"><value>ዕድመ ምስ ትውልዲ ዓ/ም ኣይጠዓዓምን </value></text>
					

					<text id="infosection2_text"><value>ክፍሊ 2፡ መረዳእታ ምቁራፅ ጥንሲ </value></text>

					<text id="q_terminationtype_text"><value>ሕ201፡ ዓይነት ምቁራፅ ጥንሲ፡ </value></text>
					<text id="q_terminationtype_abortion_spont"><value>ብባዕሉ ዝተነፀለ ጥንሲ</value></text>
					<text id="q_terminationtype_abortion_elect"><value>ብጥንቃቐ ዝተነፀለ ጥንሲ(safe abortion)</value></text>
					<text id="q_terminationtype_abortion_unsafe"><value>ዝተነኻኸአ/ጥንቃቐ ዝጎደሎ ምንፃል ጥንሲ(unsafe abortion)</value></text>
					<text id="q_terminationtype_mat_death"><value>ሞት ኣዶ</value></text>
					
					<text id="q_terminationdate_text"><value>ሕ202፡ ጥንሲ ዝተቛረፀሉ ዕለት፡</value></text>
					<text id="q_terminationdate_constraint"><value>ዝፀሓፍ ዕለት ቅድሚ ሎሚዓንቲ ወይ ሎሚዓንቲ ክኸውን ኣለዎ </value></text>
					
					<text id="q_procedure_text"><value>ሕ203፡ ኸይዲ ምንፃል ጥንሲ?፡</value></text>
					<text id="q_procedure_notapplic"><value>ኣይምልከን</value></text>
					<text id="q_procedure_dilation"><value>ከፊትካ ምፅራግ(ዳይሌሽንን ከዩሪቴጅን)</value></text>
					<text id="q_procedure_vacuum"><value>ኤም.ቪ.ኤ(MVA)</value></text>
					<text id="q_procedure_misoprostol"><value>ሚሶፕሮስቶል ተዋሂባ</value></text>
					
					<text id="q_majorcomplicat_text"><value>ሕ204፡ ቀንዲ ሳዕብየን/መሕለኽቲ፡</value></text>
					<text id="q_majorcomplicat_bleed"><value>መድመይቲ</value></text>
					<text id="q_majorcomplicat_sepsis"><value>ረኽሲ</value></text>
					<text id="q_majorcomplicat_eclampsia"><value>ምስ ጥንሲ ዝተትሓሓዘ ድፍኢት ደም</value></text>
					<text id="q_majorcomplicat_perforations"><value>ምንኳል ማህፀን</value></text>
					<text id="q_majorcomplicat_none"><value>ሳዕብየን የለን</value></text>
					
					<text id="q_referral_text"><value>ሕ205፡  ኣዶ ናብ ጥዕና ጣብያ ወይ ሆስፒታል ስልጣን ንላዕሊ ተባሂላ 'ዶ?</value></text>
					<text id="q_fpcounseled_text"><value>ሕ206፡ ስለ ምጣነ ስድራ ምኽሪ ተዋሂብዋ 'ዶ? </value></text>
					<text id="q_fpaccepted_text"><value>ሕ207፡ ምጣነ ስድራ ንምጥቃም ተስማዕሚዓ 'ዶ? </value></text>
					<text id="q_comments_text"><value>ሕ208፡ ሪኢቶ</value></text>
					
					<text id="q_gpsdata_text"><value>ሕቶ 301፡ እዚ ናይ ሕክምና ኣገልግሎት ዝተኻየደሉ ቦታ ንምምልካት ናይ ጂ.ፕ.ኤስ (GPS) ሓበሬታ ውሰዲ</value></text>

				</translation>
			</itext>
			<instance>
				<data id="Terminationv1">
					<startTime/> <!-- hidden from user -->
					<endTime/> <!-- hidden from user -->
					<deviceId/> <!-- hidden from user -->
					<today/> <!-- hidden from user -->
					<subscriberid/> <!-- hidden from user -->
					<simid/> <!-- hidden from user -->
					<phonenumber/> <!-- hidden from user -->

					<infoprotocol/>
					<q_consent/>
					<infoconsent/>
					<q_healthpointid/>
					<q_userid/>
					<q_username/>
					<q_yearofbirth/>
					<q_age/>
					
					<infosection2/>
					<q_terminationtype/>
					<q_terminationdate/>
					<q_procedure/>
					<q_majorcomplicat/>
					<q_referral/>
					<q_fpcounseled/>
					<q_fpaccepted/>
					<q_comments/>
					
					<infosection3/>
					<q_gpsdata/>
					<infoendsession/>
				</data>
			</instance>
			<bind id="startTime" nodeset="/data/startTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="start"/>
			<bind id="endTime" nodeset="/data/endTime" type="dateTime" jr:preload="timestamp" jr:preloadParams="end"/>
			<bind id="deviceId" nodeset="/data/deviceId" type="string" jr:preload="property" jr:preloadParams="deviceid"/>
			<bind id="today" nodeset="/data/today" type="date" jr:preload="date" jr:preloadParams="today"/>
			<bind id="subscriberid" nodeset="/data/subscriberid" type="string" jr:preload="property" jr:preloadParams="subscriberid"/>
			<bind id="simid" nodeset="/data/simid" type="string" jr:preload="property" jr:preloadParams="simserial"/>
			<bind id="phonenumber" nodeset="/data/phonenumber" type="string" jr:preload="property" jr:preloadParams="phonenumber"/>

			<bind id="infoprotocol" nodeset="/data/infoprotocol" readonly="true()" type="string"/>
			<bind id="q_consent" nodeset="/data/q_consent" required="true()" type="select1"/>
			<bind id="infoconsent" nodeset="/data/infoconsent" readonly="true()" type="string" relevant="selected(/data/q_consent,'no')"/>

			<bind id="q_healthpointid" nodeset="/data/q_healthpointid" required="true()" type="integer"/>
			<bind id="q_userid" nodeset="/data/q_userid" required="true()" type="integer" constraint=". &gt; 0"  jr:constraintMsg="jr:itext('q_userid_constraint')"/>
			<bind id="q_username" nodeset="/data/q_username" required="true()" type="string" />
			<bind id="q_yearofbirth" nodeset="/data/q_yearofbirth" required="true()" type="integer" constraint=". &gt; 1900" jr:constraintMsg="jr:itext('q_yearofbirth_constraint')"/>
			<!-- Constraint on age checks that the value given is roughly consistent with the (Ethiopian calendar) year of birth entered  - within around 3 years either way -->
			<bind id="q_age" nodeset="/data/q_age" required="true()" type="integer" constraint="((now() div 365.25) - (/data/q_yearofbirth -1962)) &lt; (. + 4) and ((now() div 365.25) - (/data/q_yearofbirth -1962)) &gt; (. - 3)" jr:constraintMsg="jr:itext('q_age_constraint')" />

			<bind id="infosection2" nodeset="/data/infosection2" readonly="true()" type="string"/>
			<bind id="q_terminationtype" nodeset="/data/q_terminationtype" required="true()" type="select1"/>
			<bind id="q_terminationdate" nodeset="/data/q_terminationdate" required="true()" type="date" 
					constraint="((now()-.) &gt;= 0)" 
					jr:constraintMsg="jr:itext('q_terminationdate_constraint')"
					relevant="not(selected(/data/q_terminationtype,'mat_death'))"/>
					
			<bind id="q_procedure" nodeset="/data/q_procedure" required="true()" type="select1"
					relevant="not(selected(/data/q_terminationtype,'mat_death'))"/>
					
			<bind id="q_majorcomplicat" nodeset="/data/q_majorcomplicat" required="true()" type="select1"
					relevant="not(selected(/data/q_terminationtype,'mat_death'))"/>
			
			<bind id="q_referral" nodeset="/data/q_referral" required="true()" type="select1"
					relevant="not(selected(/data/q_terminationtype,'mat_death'))"/>
					
			<bind id="q_fpcounseled" nodeset="/data/q_fpcounseled" required="true()" type="select1"
					relevant="not(selected(/data/q_terminationtype,'mat_death'))"/>
					
			<bind id="q_fpaccepted" nodeset="/data/q_fpaccepted" required="true()" type="select1"
					relevant="not(selected(/data/q_terminationtype,'mat_death')) and selected(/data/q_fpcounseled,'yes')"/>
					
			<bind id="q_comments" nodeset="/data/q_comments" type="string"/>
						
			<bind id="infosection3" nodeset="/data/infosection3" readonly="true()" type="string"/>
			<bind id="q_gpsdata" nodeset="/data/q_gpsdata" type="geopoint" format="gps" />
			<bind id="infoendsession" nodeset="/data/infoendsession" readonly="true()" type="string" 
